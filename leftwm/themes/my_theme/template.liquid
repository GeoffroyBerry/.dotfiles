{% for tag in workspace.tags %}
{% if tag.mine %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{F#000000}%{B#BD93F9}  {{tag.name}}  %{B-}%{F-}
%{A}
{% elsif tag.visible %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{F#000000}%{B#E60053}  {{tag.name}}  %{B-}%{F-}
%{A}
{% elsif tag.busy %}
%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}
%{F#BD93F9}  {{tag.name}}  %{F-}
%{A}
{% else tag.visible %}
{% comment %}%{A1:$SCRIPTPATH/change_to_tag {{workspace.index}} {{tag.index}}:}{% endcomment %}
{% comment %}%{F#FFFFFF}  {{tag.name}}  %{F-}{% endcomment %}
{% comment %}%{A}{% endcomment %}
{% endif %}
{% endfor %}
%{c}
{{ window_title }}
